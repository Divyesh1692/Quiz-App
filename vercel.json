{
    "version":2,
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/user/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/quizzes/(.*)",
            "dest":"index.js"
        }
    ]
}